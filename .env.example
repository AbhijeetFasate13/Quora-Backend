# Environment Configuration Template
# Copy this file to 'env.properties' and fill in your actual values
# NEVER commit env.properties to git!

# JWT Configuration
# Generate a secure secret: openssl rand -base64 64
JWT_SECRET=REPLACE_WITH_YOUR_GENERATED_SECRET_KEY_MINIMUM_64_CHARACTERS_BASE64_ENCODED
JWT_ACCESS_TOKEN_EXPIRATION=900000
JWT_REFRESH_TOKEN_EXPIRATION=604800000

# Database Configuration
DB_PASSWORD=your-secure-database-password
